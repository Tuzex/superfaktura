    "php": "8.1",
    "beberlei/assert": "3.3.*",
    "symfony/options-resolver": "5.*"
  },
  "require-dev": {
    "phpstan/phpstan": "1.0.*",
    "phpstan/phpstan-phpunit": "1.0.*",
    "phpunit/phpunit": "9.5.*",
    "symfony/var-dumper": "^5.3",
    "symplify/easy-coding-standard": "9.4.*"
  }
